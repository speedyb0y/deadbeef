#: ../src/main.c:139
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Kullanımı: deadbeef [seçenekler] [--] [dosya (lar)]\n"

#: ../src/main.c:140
#, c-format
msgid "Options:\n"
msgstr "Seçenekler:\n"

#: ../src/main.c:141
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  veya  -h     Yardımı yazdır (bu mesajı) ve çık\n"

#: ../src/main.c:142
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Oynatıcıyı kapat\n"

#: ../src/main.c:143
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Sürüm bilgilerini yazdır ve çık\n"

#: ../src/main.c:144
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Oynatımı başlat\n"

#: ../src/main.c:145
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Oynatımı durdur\n"

#: ../src/main.c:146
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause             Oynatımı duraklat\n"

#: ../src/main.c:147
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Duraklamayı geç\n"

#: ../src/main.c:148
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "--play-pause Durdurulmuşsa çal, değilse duraklamayı geç\n"

#: ../src/main.c:149
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Çalma listesinde sonraki parça\n"

#: ../src/main.c:150
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Çalma listesinde önceki parça\n"

#: ../src/main.c:151
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Çalma listesinde rastgele parça\n"

#: ../src/main.c:152
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Çalma listesine parça(lar) ekle\n"

#: ../src/main.c:153
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "--gui PLUGIN Hangi GUI eklentisini kullanacağını söyler, varsayılan \"GTK2\"\n"

#: ../src/main.c:154
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Değiştirilmiş dosya adını standard çıktıya (stdout) yazdır\n"

#: ../src/main.c:157
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      FMT %%-sözdizimi: [s]anatçı, [b]aşlık, al[b]üm,\n                      [u]zunluk, parça[n]umarası, [y]ıl, y[o]rum,\n                      teli[f]hakkı, [g]eçmiş\n"

#: ../src/main.c:160
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr "Örnek: --nowplaying \"%%a - %%t\"  > \"sanatçı - parça\" yazdıracak\n"

#: ../src/main.c:163
#, c-format
msgid "                      for more info, see %s\n"
msgstr "Daha fazla bilgi için, bkz: %s\n"

#: ../src/main.c:165
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      NOT: --nowplaying kulanımdan kaldırıldı.\n"

#: ../src/main.c:168
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr "--nowplaying-tf FMT Yeni başlık biçimi kullanarak biçimlendirilmiş parça adını stdout'a yazdırın\n"

#: ../src/main.c:171
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr ""

#: ../src/main.c:174
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr "örnek: --nowplaying-tf \"%%artist%% - %%title%%\"  > \"sanatçı - parça\" yazdıracak\n"

#: ../src/main.c:175
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr " --ses [NUM] Deadbeef ses seviyesini yazdırın veya ayarlayın.\n"

#: ../src/main.c:178
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr "NUM parametresi yüzde cinsinden belirtilebilir (mutlak değer veya artış/azalma)\n"

#: ../src/main.c:179
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr ""

#: ../src/main.c:180
#, c-format
msgid ""
"                      Examples: --volume 80, --volume +10, --volume -5 or "
"--volume -20dB\n"
msgstr ""

#: ../src/main.c:183
#, c-format
msgid ""
"   --plugin=[PLUG]    Send commands to a specific plugin. Use PLUG=main to "
"send commands to deadbeef itself.\n"
msgstr ""

#: ../src/main.c:184
#, c-format
msgid ""
"                      To get plugin specific commands use --plugin=[PLUG] "
"--help\n"
msgstr ""

#: ../src/main.c:187
#, c-format
msgid ""
"   --plugin-list      List all available plugins including indication for "
"plugins that support commands.\n"
msgstr ""

