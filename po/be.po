#: ../src/main.c:136
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Выкарыстаньне: deadbeef [опцыі] [--] [файл(ы)]\n"

#: ../src/main.c:137
#, c-format
msgid "Options:\n"
msgstr "Опцыі:\n"

#: ../src/main.c:138
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  або  -h    Надрукаваць даведку (гэтае паведамленьне) ды выйсьці\n"

#: ../src/main.c:139
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Выйсьці з плэера\n"

#: ../src/main.c:140
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Надрукаваць інфармацыю пра вэрсію праграмы ды выйсьці\n"

#: ../src/main.c:141
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Пачаць прайграваньне\n"

#: ../src/main.c:142
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Спыніць прайграваньне\n"

#: ../src/main.c:143
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Прыпыніць прайграваньне\n"

#: ../src/main.c:144
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Пераключыць паўзу\n"

#: ../src/main.c:145
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       Пачаць прайграваньне, калі спынена, інакш пераключыць паўзу\n"

#: ../src/main.c:146
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Наступная песьня ў плэй-лісьце\n"

#: ../src/main.c:147
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Папярэдняя песьня ў плэй-лісьце\n"

#: ../src/main.c:148
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Выпадковая песьня ў плэй-лісьце\n"

#: ../src/main.c:149
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Дадаць файл(ы) у існы плэй-ліст\n"

#: ../src/main.c:150
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "   --gui PLUGIN       Абраць убудову графічнага інтэрфэйсу; прадвызначаная «GTK2»\n"

#: ../src/main.c:151
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying ФМТ   Надрукаваць адфарматаваную назву трэка ў stdout\n"

#: ../src/main.c:152
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      %%-сынтаксіс ФМТ: [a]выканаўца, [t]назва, [b]альбом,\n                      [l]даўжыня, [n]нумар трэка, [y]год, [c]камэнтар,\n                      [r]аўтарскія правы, [e]прайшло часу\n"

#: ../src/main.c:155
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr "                      напрыклад: --nowplaying «%%a - %%t» надрукуе «выканаўца - назва»\n"

#: ../src/main.c:156
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      для дадатковых зьвестак глядзіце %s\n"

#: ../src/main.c:157
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      НАТАТКА: --nowplaying састарэлы.\n"

#: ../src/main.c:158
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr "   --nowplaying-tf ФМТ  Надрукаваць у stdout адфарматаваную назву трэка, ужываючы новы фармат загалоўка\n"

#: ../src/main.c:159
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr "                      Сінтаксіс ФМТ: http://github.com/DeaDBeeF-Player/deadbeef/wiki/Title-formatting-2.0\n"

#: ../src/main.c:160
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr "                      напрыклад: --nowplaying-tf «%%artist%% - %%title%%» надрукуе «выканаўца - назва»\n"

#: ../src/main.c:161
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr "   --volume [ЛІК]     Вывесьці або задаць узровень гучнасьці deadbeef.\n"

#: ../src/main.c:162
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr "                      Парамэтар ЛІК можа быць вызначаны ў адсотках (абсалютнае значэньне або павелічэньне ці памяншэньне)\n"

#: ../src/main.c:163
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr "                      або ў дБ [-50, 0] (калі з суфіксам).\n"

#: ../src/main.c:164
#, c-format
msgid ""
"                      Examples: --volume 80, --volume +10, --volume -5 or "
"--volume -20dB\n"
msgstr "                      Прыклады: --volume 80, --volume +10, --volume -5 або --volume -20dB\n"

#: ../src/main.c:165
#, c-format
msgid ""
"   --plugin=[PLUG]    Send commands to a specific plugin. Use PLUG=main to "
"send commands to deadbeef itself.\n"
msgstr "   --plugin=[PLUG]    Адправіць каманды да пэўнай убудовы. Ужывайце PLUG=main для адпраўкі каманд непасрэдна deadbeef.\n"

#: ../src/main.c:166
#, c-format
msgid ""
"                      To get plugin specific commands use --plugin=[PLUG] "
"--help\n"
msgstr "                      Каб даведацца пра каманды пэўнай убудовы, ужывайце --plugin=[PLUG] --help\n"

#: ../src/main.c:167
#, c-format
msgid ""
"   --plugin-list      List all available plugins including indication for "
"plugins that support commands.\n"
msgstr "   --plugin-list      Вывесьці сьпіс усіх убудоў зь індыкацыяй тых, якія падтрымліваюць каманды.\n"

