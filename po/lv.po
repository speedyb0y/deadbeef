#: ../src/main.c:134
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Lietošana: deadbeef [opcijas] [--] [fails(-i)]\n"

#: ../src/main.c:135
#, c-format
msgid "Options:\n"
msgstr "Opcijas:\n"

#: ../src/main.c:136
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  vai  -h     Parādīt palīdzību(šo ziņojumu) un iziet\n"

#: ../src/main.c:137
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Iziet no atskaņotāja\n"

#: ../src/main.c:138
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Parādīt versijas info un iziet\n"

#: ../src/main.c:139
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Sākt atskaņošanu\n"

#: ../src/main.c:140
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Apturēt atskaņošanu\n"

#: ../src/main.c:141
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Pauzēt atskaņošanu\n"

#: ../src/main.c:142
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Pārslēgt pauzi\n"

#: ../src/main.c:143
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       Sākt atskaņošanu, ja apturēts, pretējā gadījumā pārslēgt pauzi\n"

#: ../src/main.c:144
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Nākamā dziesma repertuārā\n"

#: ../src/main.c:145
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Iepriekšējā dziesma repertuārā\n"

#: ../src/main.c:146
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Nejauša dziesma repertuārā\n"

#: ../src/main.c:147
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Pievienot failu(s) esošajam repertuāram\n"

#: ../src/main.c:148
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "   --gui SPRAUDNIS       Norāda, kuru lietotāja saskarnes spraudni izmantot, \nnoklusētais ir \"GTK2\"\n"

#: ../src/main.c:149
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Standarta izvadē parādīt formatētu celiņu\n"

#: ../src/main.c:150
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      FMT %%-sintakse: [a] – izpildītājs, [t] – nosaukums,\n                      [b] – albums, [l] – ilgums, [n] – celiņa numurs,\n                      [y] – gads, [c] – komentārs, [r] – autortiesības,\n                      [e] – pagājušais laiks\n"

#: ../src/main.c:153
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr ""

#: ../src/main.c:154
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      vairāk informācijai skatiet %s\n"

#: ../src/main.c:155
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr ""

#: ../src/main.c:156
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr ""

#: ../src/main.c:157
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr ""

#: ../src/main.c:158
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr ""

#: ../src/main.c:159
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr ""

#: ../src/main.c:160
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr ""

#: ../src/main.c:161
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr ""

#: ../src/main.c:162
#, c-format
msgid ""
"                      Examples: --volume 80, --volume +10, --volume -5 or "
"--volume -20dB\n"
msgstr ""

#: ../src/main.c:163
#, c-format
msgid ""
"   --plugin=[PLUG]    Send commands to a specific plugin. Use PLUG=main to "
"send commands to deadbeef itself.\n"
msgstr ""

#: ../src/main.c:164
#, c-format
msgid ""
"                      To get plugin specific commands use --plugin=[PLUG] "
"--help\n"
msgstr ""

#: ../src/main.c:165
#, c-format
msgid ""
"   --plugin-list      List all available plugins including indication for "
"plugins that support commands.\n"
msgstr ""

