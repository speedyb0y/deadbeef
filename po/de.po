#: ../src/main.c:134
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Benutzung: deadbeef [Optionen] [--] [Datei(en)]\n"

#: ../src/main.c:135
#, c-format
msgid "Options:\n"
msgstr "Optionen:\n"

#: ../src/main.c:136
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  oder  -h   Hilfe (diese Meldung) anzeigen und beenden\n"

#: ../src/main.c:137
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             DeaDBeeF beenden\n"

#: ../src/main.c:138
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Versionsinformationen anzeigen und beenden\n"

#: ../src/main.c:139
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Wiedergabe starten\n"

#: ../src/main.c:140
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Wiedergabe beenden\n"

#: ../src/main.c:141
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Wiedergabe pausieren\n"

#: ../src/main.c:142
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Wiedergabe umschalten (Play/Pause)\n"

#: ../src/main.c:143
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       Wiedergabe starten falls angehalten, sonst pausieren\n"

#: ../src/main.c:144
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Nächster Titel in der Wiedergabeliste\n"

#: ../src/main.c:145
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Vorheriger Titel in der Wiedergabeliste\n"

#: ../src/main.c:146
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Zufälliger Titel in der Wiedergabeliste\n"

#: ../src/main.c:147
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Datei(en) an bestehende Wiedergabeliste anhängen\n"

#: ../src/main.c:148
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "--gui PLUGIN Gibt an welches GUI-Plugin benutzt werden soll, Standard ist \"GTK2\"\n"

#: ../src/main.c:149
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Formatierten Titelnamen in Standardausgabe schreiben\n"

#: ../src/main.c:150
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      FMT %%-Syntax: [a]rtist, [t]itel, Al[b]um,\n                      [l]änge, Titel[n]ummer, [y]Jahr, [c]Kommentar,\n                      copy[r]ight, [e]verstrichen\n"

#: ../src/main.c:153
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr "                      Beispiel: --nowplaying \"%%a - %%t\" ergibt \"Interpret - Titel\"\n"

#: ../src/main.c:154
#, c-format
msgid "                      for more info, see %s\n"
msgstr "für weitere Informationen siehe %s\n"

#: ../src/main.c:155
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "Anmerkung: -nowplaying ist nicht möglich\n"

#: ../src/main.c:156
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr "   --nowplaying-tf FMT  Ausgabe des formatierten Titels nach stdout, unter Verwendung des neuen Titelformats\n"

#: ../src/main.c:157
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr "                      FMT Syntax: http://github.com/DeaDBeeF-Player/deadbeef/wiki/Title-formatting-2.0\n"

#: ../src/main.c:158
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr "                      Beispiel: --nowplaying-tf \"%%artist%% - %%title%%\" ergint \"Interpret - Titel\"\n"

#: ../src/main.c:159
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr "   --volume [NUM]     Anzeige oder Setzen des Lautstärkepegels von DeaDBeeF.\n"

#: ../src/main.c:160
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr "Der NUM-Parameter kann in Prozent angegeben werden (absoluter Wert oder Anstieg/Verminderung)\n"

#: ../src/main.c:161
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr "oder in dB [-50,0] (falls mit Dateinamenerweiterung)\n"

#: ../src/main.c:162
#, c-format
msgid ""
"                      Examples: --volume 80, --volume +10, --volume -5 or "
"--volume -20dB\n"
msgstr "Beispiele: --volume 80, --volume +10, --volume -5 or --volume -20dB\n"

#: ../src/main.c:163
#, c-format
msgid ""
"   --plugin=[PLUG]    Send commands to a specific plugin. Use PLUG=main to "
"send commands to deadbeef itself.\n"
msgstr "   --plugin=[PLUG]    Kommando an eine ausgewählte Erweiterung senden. Use PLUG=main Kommando direkt an deadbeef senden.\n"

#: ../src/main.c:164
#, c-format
msgid ""
"                      To get plugin specific commands use --plugin=[PLUG] "
"--help\n"
msgstr "                      Liste ausgewählter Befehlszeilen aufrufen --plugin=[PLUG] --help\n"

#: ../src/main.c:165
#, c-format
msgid ""
"   --plugin-list      List all available plugins including indication for "
"plugins that support commands.\n"
msgstr "   --plugin-list      Alle verfügbaren Erweiterungen auflisten.\n"

