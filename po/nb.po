#: ../main.c:130
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Bruk: deadbeef [innstillinger] [-] [fil(er)]\n"

#: ../main.c:131
#, c-format
msgid "Options:\n"
msgstr "Innstillinger:\n"

#: ../main.c:132
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  eller  -h     Skriv hjelpetekst (denne teksten) og avslutt\n"

#: ../main.c:133
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Avslutt spilleren\n"

#: ../main.c:134
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Skriv versjonsinformasjon og avslutt\n"

#: ../main.c:135
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Start avspilling\n"

#: ../main.c:136
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Stopp avspilling\n"

#: ../main.c:137
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Pause avspilling\n"

#: ../main.c:138
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Slå av/på pause\n"

#: ../main.c:139
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       Start avspilling hvis stoppet, slå av/på pause ellers\n"

#: ../main.c:140
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Neste låt i spillelista\n"

#: ../main.c:141
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Forrige låt i spillelista\n"

#: ../main.c:142
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Tilfeldig låt i spillelista\n"

#: ../main.c:143
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Legg til fil(er) i eksisterende spilleliste\n"

#: ../main.c:144
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "   --gui PLUGIN       Angir brukergrensesnitt, standard er «GTK2»\n"

#: ../main.c:145
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Skriv formatert spornavn til stdout\n"

#: ../main.c:146
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      FMT %%-syntaks: [a]rtist, [t]itle, al[b]um,\n                      [l]ength, track[n]umber, [y]ear, [c]omment,\n                      copy[r]ight, [e]lapsed\n"

#: ../main.c:149
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr "                      eksempel: --nowplaying \"%%a - %%t\" vil skrive \"artist - tittel\"\n"

#: ../main.c:150
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      for mer info, se %s\n"

#: ../main.c:151
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      NB: --nowplaying er foreldet.\n"

#: ../main.c:152
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr "   --nowplaying-tf FMT  Skriv formatert spornavn to stdout, med den nye typen tittelformatering\n"

#: ../main.c:153
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr "                      FMT-syntaks: http://github.com/DeaDBeeF-Player/deadbeef/wiki/Title-formatting-2.0\n"

#: ../main.c:154
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr "                      eksempel: --nowplaying-tf \"%%artist%% - %%title%%\" vil skrive \"artist - tittel\"\n"

#: ../main.c:155
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr "   --volume [NUM]     Skriv eller velg deadbeefs volumnivå.\n"

#: ../main.c:156
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr "                      NUM-parameteren kan angis i prosent (absoluttverdi eller økning/reduksjon)\n"

#: ../main.c:157
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr "                      eller i dB [-50, 0] (bruk i så fall suffiks).\n"

#: ../main.c:158
#, c-format
msgid ""
"                      Examples: --volume 80, --volume +10, --volume -5 or "
"--volume -20dB\n"
msgstr "                      Eksempler: --volume 80, --volume +10, --volume -5 eller --volume -20dB\n"

#: ../main.c:159
#, c-format
msgid ""
"   --plugin=[PLUG]    Send commands to a specific plugin. Use PLUG=main to "
"send commands to deadbeef itself.\n"
msgstr "   --plugin=[PLUG]    Send kommando til valgt tillegg. Velg PLUG=main for å sende kommandoer til selve deadbeef.\n"

#: ../main.c:160
#, c-format
msgid ""
"                      To get plugin specific commands use --plugin=[PLUG] "
"--help\n"
msgstr "                      Bruk --plugin=[PLUG] --help for å liste ut kommandoer som er spesifikke for tillegget\n"

#: ../main.c:161
#, c-format
msgid ""
"   --plugin-list      List all available plugins including indication for "
"plugins that support commands.\n"
msgstr "   --plugin-list      Lister ut alle tilgjengelige tillegg og om de støtter kommandoer.\n"

